{
  "name": "csv-extractor",
  "private": true,
  "version": "1.1.1",
  "description": "Webservice for extracting CSV data atoms and putting them on a message bus.",
  "repository": "https://github.com/menome",
  "license": "Commercial",
  "dependencies": {
    "@menome/botframework": "^3.1.0",
    "csvtojson": "^2.0.8"
  },
  "devDependencies": {
    "mocha": "^5.1.1",
    "chai": "^4.1.0",
    "nodemon": "^1.18.4",
    "mock-require": "^2.0.1"
  },
  "scripts": {
    "start": "[ \"$NODE_ENV\" = \"production\" ] && npm run prod || npm run dev",
